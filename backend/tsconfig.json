{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "controllers/*": ["./src/controllers/*"],
      "libs/*": ["./src/libs/*"],
      "models/*": ["./src/models/*"],
      "routes/*": ["./src/routes/*"],
      "middlewares/*": ["./src/middlewares/*"]
    }
  }
}